pipeline {
    agent any
    stages {
        stage("Build") {
            steps {
                echo "===executing build==="
            }
            post {
                success {
                    mail to: "jakebennett1112@gmail.com",
                    subject: "Build status email",
                    body: "Build was successful"
                }
            }
        }
        stage ("Complete){
            steps{
                echo "Completed."
        }
    }
}
